<fetch version="1.0" output-format="xml-platform" mapping="logical" distinct="true">
  <entity name="balbilal_subscription">
    <attribute name="balbilal_subscriptionid" />
    <attribute name="balbilal_telecomsubscriptionid" />
    <attribute name="createdon" />
    <order attribute="balbilal_telecomsubscriptionid" descending="false" />
    <filter type="and">
      <condition attribute="balbilal_paymentplan" operator="eq" value="" />
    </filter>
    <link-entity name="balbilal_billing" from="balbilal_subscriptionidlp" to="balbilal_subscriptionid" link-type="inner" alias="ah">
      <filter type="and">
        <condition attribute="balbilal_billinginterval" operator="eq" value="" />
      </filter>
    </link-entity>
  </entity>
</fetch>